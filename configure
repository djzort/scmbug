  DEVEL_CONFIG_LOG_TEMPLATE_BUGID_REGEX='^\s*bug\s*([\d|\s|,|#]*?):'
fi



DEVEL_CONFIG_LOG_TEMPLATE_BUGID_SPLIT_REGEX_OUTSIDE=",\s?#|\s?#|,|\s+"

